<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   android:paddingLeft="5dp"
    tools:context="com.example.abhujeeth.project_was.MainActivity"
    android:orientation="vertical"
    android:background="@color/colorfour">

    <fragment
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:id="@+id/frag1"
        android:name="com.example.abhujeeth.project_was.BlankFragment"
        android:layout_marginTop="2dp"
        android:layout_weight=".2"/>


    <fragment
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:id="@+id/frag2"
        android:name="com.example.abhujeeth.project_was.FragmentFirst"
        android:layout_weight=".35"/>

    <fragment
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:id="@+id/frag3"
        android:name="com.example.abhujeeth.project_was.FragmentSecond"
        android:layout_marginTop="1px"
        android:layout_weight=".45"/>


</LinearLayout>
